{
  "manifest_version": 2,
  "name": "exercism-firacode",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": [
        "https://exercism.io/*"
      ],
      "css": ["exercism.css"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "FiraCode/*.eot",
    "FiraCode/*.eot",
    "FiraCode/*.woff2",
    "FiraCode/*.woff",
    "FiraCode/*.ttf"
  ],
  "permissions": ["activeTab"],
  "description": "A chrome extension that brings FiraCode font to Exercism's code blocks. Modified directly from github-firacode.  Special thanks to Jan Břečka!",
  "icons": {
    "128": "images/exercism.png"
  }
}
